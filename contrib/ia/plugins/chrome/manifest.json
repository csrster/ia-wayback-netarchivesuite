{
   "name": "Wayback Chrome Plugin",
   "browser_action": {
     "default_icon": "wb48.png",
     "default_title": "Wayback Machine",
     "default_popup": "popup.html"
   },
   
   "icons": {"48": "wb48.png", "128": "wb128.png"},
  
   "version": "1.0",
   "manifest_version": 2,
   "background": {
       "scripts": ["wayback.js"]
   },
   "permissions": [
       "webRequest",
       "webRequestBlocking",
       "<all_urls>",
       "tabs",
       "proxy"
   ]
}
